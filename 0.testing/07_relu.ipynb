    "# NumPyの読み込み\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "# ReLU関数の定義\n",
    "def relu(x):\n",
    "    return np.maximum(x, 0)\n",
    "    \n",
    "\n",
    "\n",
    "# ReLU関数の出力\n",
    "print(\"ReLU関数の出力\")\n",
    "print(relu(-5))\n",
    "print(relu(0))\n",
    "print(relu(5))\n",
    "print()"
   ]
